{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:read-files",
    "fs:write-files",
    {
      "identifier": "fs:scope",
      "allow": [
        "$HOME/**"
      ]
    }
  ]
}
